<h1>Listing Foods</h1>
<%= link_to 'New Food', new_food_path %>
<table>
  <tr>
    <th>Name</th>
    <th>Text</th>
  </tr>

  <% @foods.each do |food| %>
      <tr>
        <td><%= food.name %></td>
        <td><%= food.text %></td>
        <td><%= link_to 'Show', food_path(food) %></td>
        <td><%= link_to 'Edit', edit_food_path(food) %></td>
      </tr>
  <% end %>
</table>